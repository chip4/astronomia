{
  "name": "astronomia",
  "version": "2.1.3",
  "description": "An astronomical library",
  "keywords": [
    "astronomy",
    "julian",
    "meeus",
    "planetposition"
  ],
  "homepage": "https://github.com/commenthol/astronomia",
  "bugs": {
    "url": "https://github.com/commenthol/astronomia/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commenthol/astronomia.git"
  },
  "license": "MIT",
  "author": "commenthol <commenthol@gmail.com>",
  "contributors": [],
  "main": "lib",
  "module": "es",
  "jsnext:main": "es",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm test",
    "build": "npm run build:cjs && npm run build:es5",
    "build:cjs": "babel -d lib src",
    "build:es5": "BABEL_ENV=es5 babel -d es src",
    "build:docs": "jsdoc2md -f 'src/**/*.js' > docs/jsdoc.md",
    "clean": "rimraf lib es coverage .nyc_output",
    "clean:all": "rimraf node_modules && npm run clean",
    "coverage": "nyc -r text -r html npm test",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "npm run all",
    "test": "mocha",
    "zuul": "zuul test/*.js"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env"
        ]
      },
      "es5": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current",
                "browsers": [
                  "last 2 versions",
                  "ie >= 11"
                ]
              },
              "modules": false,
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "standard"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "babelify": "^10.0.0",
    "core-js": "^3.6.5",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "^3.0.2",
    "serialize-to-js": "^3.1.1",
    "serialize-to-module": "^1.0.1",
    "zuul": "^3.12.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "maintainers": [
    "commenthol <commenthol@gmail.com>"
  ]
}
